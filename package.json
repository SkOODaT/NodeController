{
  "name": "nodecontroller",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "TS_NODE_FILES=true node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "start:production": "cd dist && node -r ts-node/register src/index.js",
    "build": "tsc -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versx/NodeController.git"
  },
  "author": "versx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/versx/NodeController/issues"
  },
  "homepage": "https://github.com/versx/NodeController#readme",
  "dependencies": {
    "@types/request": "^2.48.4",
    "express": "^4.17.1",
    "google-protobuf": "^3.11.2",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "nodes2ts": "^2.0.0",
    "promisify": "0.0.3",
    "protobufjs": "^6.8.8",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "turf": "^3.0.14",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/mysql": "^2.15.8",
    "@types/node": "^13.1.6",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "pogo_protos"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
